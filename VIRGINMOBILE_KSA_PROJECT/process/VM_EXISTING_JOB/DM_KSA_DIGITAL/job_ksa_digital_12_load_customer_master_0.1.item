    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;&#9;INSERT&#9;INTO&#13;&#10;&#9;&#9;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.digital_customer_master &#13;&#10;&#9;SELECT&#13;&#10;&#9;&#9;&#9;a.Reg_ID,&#13;&#10;&#9;&#9;&#9;a.Reg_UID,&#13;&#10;&#9;&#9;&#9;e.customer_inaccount_number,&#13;&#10;&#9;&#9;&#9;a.Reg_Email,&#13;&#10;&#9;&#9;&#9;a.ChannelID,&#13;&#10;&#9;&#9;&#9;b.BookedNumber,&#13;&#10;&#9;&#9;&#9;b.Vanity_Desc,&#13;&#10;&#9;&#9;&#9;c.first_subscription_details,&#13;&#10;&#9;&#9;&#9;NVL(c.first_subscription_amount,0) AS first_subscription_amount,&#13;&#10;&#9;&#9;&#9;NVL(d.first_payment_amount,0) AS first_payment_amount,&#13;&#10;&#9;&#9;&#9;d.first_payment_order_description,&#13;&#10;&#9;&#9;&#9;d.first_payment_service_type,&#13;&#10;&#9;&#9;&#9;d.payment_initiator,&#13;&#10;&#9;&#9;&#9;d.payment_method,&#13;&#10;&#9;&#9;&#9;d.brand,&#13;&#10;&#9;&#9;&#9;d.bank,&#13;&#10;&#9;&#9;&#9;d.card_type,&#13;&#10;&#9;&#9;&#9;d.Country,&#13;&#10;&#9;&#9;&#9;e.SubscriptionType,&#13;&#10;&#9;&#9;&#9;e.ID_Type ,&#13;&#10;&#9;&#9;&#9;e.ID_Nationality,&#13;&#10;&#9;&#9;&#9;e.IMSI,&#13;&#10;&#9;&#9;&#9;e.MSISDN,&#13;&#10;&#9;&#9;&#9;e.DistributorCode,&#13;&#10;&#9;&#9;&#9;e.DealerCode,&#13;&#10;&#9;&#9;&#9;e.ActivationChannel,&#13;&#10;&#9;&#9;&#9;e.QualityGrade,&#13;&#10;&#9;&#9;&#9;e.RejectionReason,&#13;&#10;&#9;&#9;&#9;to_number(e.Longitude) AS Longitude,&#13;&#10;&#9;&#9;&#9;to_number(e.Latitude) AS Latitude ,&#13;&#10;&#9;&#9;&#9;cast(a.IsEmailVerified_Flag AS int) AS Customer_Email_Verification_Flag,&#13;&#10;&#9;&#9;&#9;a.Registration_Flag AS Customer_Registration_Flag,&#13;&#10;&#9;&#9;&#9;nvl(b.booked_number_flag,0) AS Customer_Booked_Number_Flag,&#13;&#10;&#9;&#9;&#9;nvl(c.FIRST_SUBSCRIPTION_FLAG,0) AS Subscription_First_Flag,&#13;&#10;&#9;&#9;&#9;nvl(d.first_payment_flag,0) AS Payment_First_Flag,&#13;&#10;&#9;&#9;&#9;nvl(e.activation_flag,0) AS Customer_activation_flag,&#13;&#10;&#9;&#9;&#9;c.first_subscription_timestamp,&#13;&#10;&#9;&#9;&#9;e.activation_timestamp,&#13;&#10;&#9;&#9;&#9;b.ReservationTime,&#13;&#10;&#9;&#9;&#9;a.registration_timestamp,&#13;&#10;&#9;&#9;&#9;d.first_payment_timestamp,&#13;&#10;&#9;&#9;&#9;CURRENT_TIMESTAMP AS customer_snapshot_date,&#13;&#10;&#9;&#9;&#9;f.vanity_details_occurrence,&#13;&#10;&#9;&#9;&#9;f.Reserve_Number_occurrence,&#13;&#10;&#9;&#9;&#9;f.Available_Plans_By_Product_Code_occurrence,&#13;&#10;&#9;&#9;&#9;f.Confirm_Online_Payment_Transaction_occurrence,&#13;&#10;&#9;&#9;&#9;f.Pay_W_Registration_occurrence,&#13;&#10;&#9;&#9;&#9;f.Confirm_Registration_Pay_Fort_occurrence,&#13;&#10;&#9;&#9;&#9;f.vanity_first_datetime,&#13;&#10;&#9;&#9;&#9;f.Reserve_Number_first_datetime,&#13;&#10;&#9;&#9;&#9;f.Available_Plans_By_Product_Code_first_datetime,&#13;&#10;&#9;&#9;&#9;f.Confirm_Online_Payment_Transaction_first_datetime,&#13;&#10;&#9;&#9;&#9;f.Pay_W_Registration_First_datetime,&#13;&#10;&#9;&#9;&#9;f.Confirm_Registration_Pay_Fort_first_datetime,&#13;&#10;&#9;&#9;&#9;CASE WHEN a.Password IS NOT NULL THEN 1&#9;ELSE 0&#9;END AS Digital_Customer_flag,&#13;&#10;&#9;&#9;&#9;CASE WHEN a.Reg_Email IS NOT NULL THEN 1 ELSE 0&#9;END AS Account_Created_flag,&#13;&#10;&#9;&#9;&#9;CASE WHEN confirm_login_flag_first IS NOT NULL THEN 1 ELSE 0 END AS Login_Flag,&#13;&#10;&#9;&#9;&#9;CASE WHEN a.Password IS NOT NULL AND nvl(e.activation_flag,0) = 1 AND e.Activation_Timestamp &gt;= a.Registration_Timestamp THEN 1&#9;ELSE 0 END AS Digital_Activation_Flag,&#13;&#10;&#9;&#9;&#9;CASE WHEN a.Password IS NOT NULL AND e.Activation_Timestamp &lt;a.Registration_Timestamp THEN 1 ELSE  0 END AS Digital_Migration_Flag&#13;&#10;&#9;&#9;FROM&#13;&#10;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;SELECT &#13;&#10;&#9;&#9;&#9;&#9;reg.ID AS Reg_ID,&#13;&#10;&#9;&#9;&#9;&#9;uid.Email AS Reg_Email,&#13;&#10;&#9;&#9;&#9;&#9;registration_timestamp,&#13;&#10;&#9;&#9;&#9;&#9;NVL(NVL(AccountNumber,'0')::!int,0) AS Reg_UID,&#13;&#10;&#9;&#9;&#9;&#9;reg.ChannelID,&#13;&#10;&#9;&#9;&#9;&#9;IsEmailVerified AS IsEmailVerified_Flag,&#13;&#10;&#9;&#9;&#9;&#9;reg.Registration_Flag,&#13;&#10;&#9;&#9;&#9;&#9;uid.Password&#13;&#10;&#9;&#9;&#9;FROM&#13;&#10;&#9;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;&#9;&#9;SELECT &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;ID,&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;ChannelID,&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;datetime AS Registration_Timestamp,&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;1 AS registration_flag&#13;&#10;&#9;&#9;&#9;&#9;&#9;FROM&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.digital_tx_registrations&#13;&#10;&#9;&#9;&#9;)reg&#13;&#10;&#9;&#9;&#9;LEFT JOIN &#13;&#10;&#9;&#9;&#9;(&#9;SELECT &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;b.AccountNumber,&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;b.id,&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;email,&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;Password,&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;ChannelID,&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;IsEmailVerified&#13;&#10;&#9;&#9;&#9;&#9;&#9;FROM&#13;&#10;&#9;&#9;&#9;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;SELECT &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;AccountNumber,&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;max(id) AS id&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;FROM&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.digital_tx_registrations&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;GROUP BY&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;AccountNumber &#13;&#10;&#9;&#9;&#9;&#9;&#9;)a&#13;&#10;&#9;&#9;&#9;&#9;&#9;LEFT JOIN &#13;&#10;&#9;&#9;&#9;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;SELECT *&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;FROM&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.digital_tx_registrations&#13;&#10;&#9;&#9;&#9;&#9;&#9;)b &#13;&#10;&#9;&#9;&#9;&#9;&#9;ON&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;a.id = b.id&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;AND a.AccountNumber = b.AccountNumber&#13;&#10;&#9;&#9;&#9;)uid &#13;&#10;&#9;&#9;&#9;ON&#13;&#10;&#9;&#9;&#9;&#9;reg.id = uid.id &#13;&#10;&#9;&#9;) a&#13;&#10;&#9;&#9;LEFT JOIN &#13;&#10;&#9;&#9;(&#9;SELECT&#13;&#10;&#9;&#9;&#9;&#9;Regid AS Reg_ID,&#13;&#10;&#9;&#9;&#9;&#9;BookedNumber,&#13;&#10;&#9;&#9;&#9;&#9;ReservationTime,&#13;&#10;&#9;&#9;&#9;&#9;Vanity_Desc,&#13;&#10;&#9;&#9;&#9;&#9;booked_number_flag&#13;&#10;&#9;&#9;&#9;FROM&#13;&#10;&#9;&#9;&#9;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.digital_tx_booked_numbers_current_status&#13;&#10;&#9;&#9;) b &#13;&#10;&#9;&#9;ON&#13;&#10;&#9;&#9;&#9;a.Reg_ID = b.Reg_ID&#13;&#10;&#9;&#9;LEFT JOIN &#13;&#10;&#9;&#9;(&#9;SELECT&#13;&#10;&#9;&#9;&#9;&#9;SUBSCRIPTION_UID,&#13;&#10;&#9;&#9;&#9;&#9;SUBSCRIPTION_EMAIL,&#13;&#10;&#9;&#9;&#9;&#9;FIRST_SUBSCRIPTION_FLAG,&#13;&#10;&#9;&#9;&#9;&#9;FIRST_SUBSCRIPTION_TIMESTAMP,&#13;&#10;&#9;&#9;&#9;&#9;FIRST_SUBSCRIPTION_DETAILS,&#13;&#10;&#9;&#9;&#9;&#9;FIRST_SUBSCRIPTION_AMOUNT&#13;&#10;&#9;&#9;&#9;FROM&#13;&#10;&#9;&#9;&#9;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.digital_tx_first_subscription_details&#13;&#10;&#9;&#9;&#9;WHERE&#13;&#10;&#9;&#9;&#9;&#9;FIRST_SUBSCRIPTION_FLAG = 1&#13;&#10;&#9;&#9;) c &#13;&#10;&#9;&#9;ON&#13;&#10;&#9;&#9;&#9;a.Reg_UID = c.SUBSCRIPTION_UID&#13;&#10;&#9;&#9;LEFT JOIN &#13;&#10;&#9;&#9;(&#9;SELECT&#13;&#10;&#9;&#9;&#9;&#9;ap.payment_uid,&#13;&#10;&#9;&#9;&#9;&#9;ap.PAYMENT_USEREMAIL,&#13;&#10;&#9;&#9;&#9;&#9;ap.First_Payment_Flag,&#13;&#10;&#9;&#9;&#9;&#9;ap.First_Payment_Amount,&#13;&#10;&#9;&#9;&#9;&#9;ap.First_Payment_Order_Description,&#13;&#10;&#9;&#9;&#9;&#9;ap.First_Payment_Service_type,&#13;&#10;&#9;&#9;&#9;&#9;ap.First_Payment_Timestamp,&#13;&#10;&#9;&#9;&#9;&#9;ap.payment_initiator,&#13;&#10;&#9;&#9;&#9;&#9;ap.payment_method,&#13;&#10;&#9;&#9;&#9;&#9;bp.brand,&#13;&#10;&#9;&#9;&#9;&#9;bp.bank,&#13;&#10;&#9;&#9;&#9;&#9;bp._type AS card_type,&#13;&#10;&#9;&#9;&#9;&#9;bp.country_3 AS Country&#13;&#10;&#9;&#9;&#9;FROM&#13;&#10;&#9;&#9;&#9;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.digital_tx_first_payment_details ap&#13;&#10;&#9;&#9;&#9;LEFT JOIN &quot;+context.connection_verticadb_schema_prod+&quot;.digital_dim_card_bin_codes bp &#13;&#10;&#9;&#9;&#9;ON&#13;&#10;&#9;&#9;&#9;&#9;SUBSTRING(ap.Payment_card_num, 1, 6) = bp.bin&#13;&#10;&#9;&#9;&#9;WHERE&#13;&#10;&#9;&#9;&#9;&#9;First_Payment_Flag = 1&#13;&#10;&#9;&#9;) d &#13;&#10;&#9;&#9;ON&#13;&#10;&#9;&#9;&#9;a.Reg_UID = d.payment_uid&#13;&#10;&#9;&#9;LEFT JOIN &#13;&#10;&#9;&#9;(&#9;SELECT&#13;&#10;&#9;&#9;&#9;&#9;CreatedOn AS activation_timestamp,&#13;&#10;&#9;&#9;&#9;&#9;NobillSubscriptionType AS SubscriptionType,&#13;&#10;&#9;&#9;&#9;&#9;1 AS activation_flag,&#13;&#10;&#9;&#9;&#9;&#9;ID_Type,&#13;&#10;&#9;&#9;&#9;&#9;ID_Nationality,&#13;&#10;&#9;&#9;&#9;&#9;IMSI,&#13;&#10;&#9;&#9;&#9;&#9;MSISDN,&#13;&#10;&#9;&#9;&#9;&#9;DistributorCode,&#13;&#10;&#9;&#9;&#9;&#9;DealerCode,&#13;&#10;&#9;&#9;&#9;&#9;NobillAccountNumber AS customer_inaccount_number,&#13;&#10;&#9;&#9;&#9;&#9;ActivationChannel,&#13;&#10;&#9;&#9;&#9;&#9;QualityGrade,&#13;&#10;&#9;&#9;&#9;&#9;RejectionReason,&#13;&#10;&#9;&#9;&#9;&#9;CRM_AccountID,&#13;&#10;&#9;&#9;&#9;&#9;Longitude,&#13;&#10;&#9;&#9;&#9;&#9;Latitude&#13;&#10;&#9;&#9;&#9;FROM&#13;&#10;&#9;&#9;&#9;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.cas_activations_ltd&#13;&#10;&#9;&#9;) e &#13;&#10;&#9;&#9;ON&#13;&#10;&#9;&#9;&#9;e.CRM_AccountID = a.Reg_UID&#13;&#10;&#9;&#9;LEFT JOIN &#13;&#10;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;SELECT&#13;&#10;&#9;&#9;&#9;&#9;*&#13;&#10;&#9;&#9;&#9;FROM&#13;&#10;&#9;&#9;&#9;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.stg_reg_digital_flags &#13;&#10;&#9;&#9;) AS f &#13;&#10;&#9;&#9;ON&#13;&#10;&#9;&#9;&#9;f.regID = a.Reg_ID&quot;"/>
