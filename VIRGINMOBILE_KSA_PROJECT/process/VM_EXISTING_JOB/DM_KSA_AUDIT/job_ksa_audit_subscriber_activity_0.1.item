    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;INSERT INTO &quot;+context.connection_verticadb_schema_audit+&quot;.nobill_audit_subscriber_activity&#13;&#10;SELECT &#13;&#10;&#9;curr.ad_date AS ad_date,&#13;&#10;&#9;prev.total_uds_a1d_flag AS opening_1d_base,&#13;&#10;&#9;curr.total_uds_a1d_flag AS closing_1d_base,&#13;&#10;&#9;curr.total_uds_1d_flag_churn AS churn_1d,&#13;&#10;&#9;curr.total_1d_new_or_reactivated AS new_reactivated_1d,&#13;&#10;&#9;--closing_base = opening_base - churn_1d + new_reactivated_1d&#13;&#10;&#9;prev.total_uds_a1d_flag - curr.total_uds_1d_flag_churn + curr.total_1d_new_or_reactivated - curr.total_uds_a1d_flag AS a1d_variance,&#13;&#10;&#9;prev.total_uds_a30d_flag AS opening_30d_base,&#13;&#10;&#9;curr.total_uds_a30d_flag AS closing_30d_base,&#13;&#10;&#9;curr.total_uds_30d_flag_churn AS churn_30d,&#13;&#10;&#9;curr.total_30d_new_or_reactivated AS new_reactivated_30d,&#13;&#10;&#9;--closing_base = opening_base - churn_30d + new_reactivated_30d&#13;&#10;&#9;prev.total_uds_a30d_flag - curr.total_uds_30d_flag_churn + curr.total_30d_new_or_reactivated - curr.total_uds_a30d_flag AS a30d_variance&#13;&#10;FROM&#9;&#13;&#10;(SELECT &#13;&#10;&#9;ad_date,&#13;&#10;&#9;SUM(ad_flag_active_1d) total_uds_a1d_flag,&#13;&#10;&#9;SUM(ad_flag_churn_1d) total_uds_1d_flag_churn,&#13;&#10;&#9;SUM(CASE WHEN ad_period_activity =1 and (ad_period_active_previous is null or ad_period_active_previous&lt;&gt;1 ) THEN 1 ELSE 0 END) total_1d_new_or_reactivated,&#13;&#10;&#9;SUM(ad_flag_active_30d) total_uds_a30d_flag,&#13;&#10;&#9;SUM(ad_flag_churn_30d) total_uds_30d_flag_churn,&#13;&#10;&#9;SUM(CASE WHEN ad_period_activity &gt; 0 and ad_period_activity &lt;= 30 and (ad_period_active_previous is null or ad_period_active_previous&gt;30 or  ad_period_active_previous=0) THEN 1 ELSE 0 END) total_30d_new_or_reactivated&#13;&#10;FROM&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.nobill_activity_subscriber_daily &#13;&#10;WHERE&#13;&#10;&#9;ad_date &gt;= '&quot;+context.vStartDate+&quot;'&#13;&#10;&#9;and ad_date &lt;= '&quot;+context.vEndDate+&quot;'&#13;&#10;GROUP BY &#13;&#10;&#9;1) curr&#13;&#10;LEFT JOIN&#13;&#10;(SELECT &#13;&#10;&#9;ad_date,&#13;&#10;&#9;SUM(ad_flag_active_1d) total_uds_a1d_flag,&#13;&#10;&#9;SUM(ad_flag_active_30d) total_uds_a30d_flag&#13;&#10;FROM&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.nobill_activity_subscriber_daily &#13;&#10;WHERE&#13;&#10;&#9;ad_date &gt;= date('&quot;+context.vStartDate+&quot;' )-1&#13;&#10;&#9;and ad_date &lt;= date('&quot;+context.vEndDate+&quot;')&#13;&#10;GROUP BY &#13;&#10;&#9;1) prev&#13;&#10;ON&#13;&#10;&#9;DATE(curr.ad_date)=DATE(prev.ad_date) + 1 &#13;&#10;ORDER BY &#13;&#10;&#9;curr.ad_date&quot;"/>
