THEN 1 ELSE 0 END) AS re_second_recharge_flag,&#13;&#10;&#9;&#9;SUM(CASE WHEN recharge_rank=3 THEN 1 ELSE 0 END) AS re_third_recharge_flag,&#13;&#10;&#9;&#9;SUM(CASE WHEN recharge_rank=4 THEN 1 ELSE 0 END) AS re_fourth_recharge_flag,&#13;&#10;&#9;&#9;SUM(CASE WHEN recharge_rank=5 THEN 1 ELSE 0 END) AS re_fifth_recharge_flag,&#13;&#10;&#9;&#9;SUM(CASE WHEN recharge_rank=6 THEN 1 ELSE 0 END) AS re_sixth_recharge_flag,&#13;&#10;&#9;&#9;SUM(fad_quantity) as re_total_recharge_count,&#13;&#10;&#9;&#9;SUM(fad_amount) as re_total_recharge_amount,&#13;&#10;&#9;&#9;SUM(CASE WHEN fad_date = '&quot;+context.vLoadDate+&quot;' THEN 1 ELSE 0 END) as re_daily_recharge_count,&#13;&#10;&#9;&#9;SUM(CASE WHEN fad_date = '&quot;+context.vLoadDate+&quot;' THEN fad_amount ELSE 0 END) as re_daily_recharge_amount,&#13;&#10;&#9;&#9;SUM(CASE WHEN fad_date &gt;= '&quot;+context.vPrev30LoadDate+&quot;' THEN 1 ELSE 0 END) as re_30d_recharge_count,&#13;&#10;&#9;&#9;SUM(CASE WHEN fad_date &gt;= '&quot;+context.vPrev30LoadDate+&quot;' THEN fad_amount ELSE 0 END) as re_30d_recharge_amount,&#13;&#10;&#9;&#9;SUM(CASE WHEN to_char(fad_date,'YYYYMM') &gt;= '&quot;+context.vLoadMonth+&quot;' THEN 1 ELSE 0 END) as re_monthly_recharge_count,&#13;&#10;&#9;&#9;SUM(CASE WHEN to_char(fad_date,'YYYYMM') &gt;= '&quot;+context.vLoadMonth+&quot;' THEN fad_amount ELSE 0 END) as re_monthly_recharge_amount&#13;&#10;&#9;FROM&#13;&#10;&#9;&#9;(SELECT&#13;&#10;&#9;&#9;&#9;fad_accountnum,&#13;&#10;&#9;&#9;&#9;fad_quantity,&#13;&#10;&#9;&#9;&#9;fad_subscription_type_id,&#13;&#10;&#9;&#9;&#9;fad_date,&#13;&#10;&#9;&#9;&#9;fad_time_stamp,&#13;&#10;&#9;&#9;&#9;ROW_NUMBER() OVER (PARTITION BY fad_accountnum ORDER BY fad_time_stamp) as recharge_rank,&#13;&#10;&#9;&#9;&#9;zeroifnull(fad_amount) as fad_amount&#13;&#10;&#9;&#9;FROM&#13;&#10;&#9;&#9;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.nobill_fact_adjustment,&#13;&#10;&#9;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.nobill_dim_adjustment_type&#1