s_portIn_activation_flag,&#13;&#10;&#9;IsEsimActivation AS is_eSim_activation_flag,&#13;&#10;&#9;IsPortedOut AS is_portedOut_flag,&#13;&#10;&#9;ICCID AS iccid,&#13;&#10;&#9;PUK AS puk,&#13;&#10;&#9;IdentityMasterId&#13;&#10; FROM&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.cas_activations_ltd &#13;&#10; WHERE &#13;&#10;&#9;DATE(CreatedOn) = '&quot;+context.vLoadDate+&quot;' &#13;&#10; ORDER BY &#13;&#10;&#9;NobillAccountNumber&#9;) CAS&#13;&#10;LEFT JOIN&#9;&#13;&#10;(SELECT &#13;&#10;&#9;accountnum,&#13;&#10;&#9;CASE WHEN status=2 THEN 'BEFORE FIRST USE'&#13;&#10;&#9;&#9; WHEN status=3 THEN 'IN USE' &#13;&#10;&#9;&#9; WHEN status=4 THEN 'COOLING' &#13;&#10;&#9;&#9; WHEN status=5 THEN 'COLD' &#13;&#10;&#9;&#9; WHEN status=6 THEN 'FROZEN' &#13;&#10;&#9;&#9; WHEN status=7 THEN 'TERMINATED' ELSE 'UNKNOWN' END AS nobill_status&#13;&#10; FROM&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.nobill_snapshot_subscription&#13;&#10; WHERE &#13;&#10;&#9;DATE(snapshotdate) = DATE( '&quot;+context.vLoadDate+&quot;')+1&#13;&#10; ORDER BY&#13;&#10;&#9;accountnum) SNAP&#9;&#13;&#10;ON &#13;&#10;&#9;CAS.accountnum = SNAP.accountnum&#13;&#10;ORDER BY &#13;&#10;&#9;CAS.IdentityMasterId) OUT1&#13;&#10;LEFT JOIN&#13;&#10;(SELECT&#13;&#10;&#9;Id,&#13;&#10;&#9;IdNumber AS customer_id_number,&#13;&#10;&#9;FirstName AS customer_firstname,&#13;&#10;&#9;LastName AS customer_lastname,&#13;&#10;&#9;BorderNumber AS customer_border_number&#13;&#10; FROM&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.cas_id_master_ltd CID&#13;&#10; ORDER BY &#13;&#10;&#9;Id) CID&#9;&#13;&#10;ON &#13;&#10;&#9;OUT1.IdentityMasterId = CID.Id&#9;&quot;"/>
