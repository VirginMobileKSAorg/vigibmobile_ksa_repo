    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;TRUNCATE TABLE &quot;+context.connection_verticadb_schema_stg+&quot;.nobill_dim_customer_candidates;&#13;&#10;&#13;&#10;INSERT INTO &quot;+context.connection_verticadb_schema_stg+&quot;.nobill_dim_customer_candidates&#13;&#10;SELECT&#13;&#10;&#9;OUT2.*&#13;&#10;FROM &#13;&#10;(&#13;&#10;SELECT &#13;&#10;&#9;ACT.ActivationID,&#13;&#10;&#9;ACT.NobillAccountNumber,&#13;&#10;&#9;OUT1.scd_start AS scd_start, &#13;&#10;&#9;OUT1.row_change_reason AS row_change_reason,&#13;&#10;&#9;CASE WHEN OUT1.row_change_reason IN ('ID CHANGE', 'OWNER CHANGE') AND OUT1.updated_value1 IS NULL THEN ACT.IdentityMasterId::varchar ELSE OUT1.updated_value1 END AS updated_value1,&#13;&#10;&#9;OUT1.updated_value2 AS updated_value2,&#13;&#10;&#9;NULL AS updated_value3,&#13;&#10;&#9;0 AS rank_status&#13;&#10;FROM&#13;&#10;(SELECT &#13;&#10;&#9;ID AS ActivationID,&#13;&#10;&#9;NobillAccountNumber,&#13;&#10;&#9;IdentityMasterId&#13;&#10;FROM &#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.cas_activations_ltd&#13;&#10;ORDER BY &#13;&#10;&#9;ID ) ACT&#13;&#10;INNER JOIN &#13;&#10;(SELECT &#13;&#10;&#9;OWN1.ActivationID AS ActivationID, &#13;&#10;&#9;OWN1.ChangeTimestamp AS scd_start, &#13;&#10;&#9;CASE WHEN OWN1.ProcessName='UpdateID' THEN 'ID CHANGE' &#13;&#10;&#9;&#9;      WHEN OWN1.ProcessName='ChangeOwnership' THEN  'OWNER CHANGE' END AS row_change_reason,&#13;&#10;&#9;OWN3.NextIdentityMaster::VARCHAR AS updated_value1,&#13;&#10;&#9;NULL AS updated_value2&#13;&#10;FROM&#13;&#10;(SELECT &#13;&#10;&#9;ActivationID, ChangeTimestamp, ProcessName&#13;&#10;FROM &#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.cas_changeowner_ltd WHERE date(ChangeTimestamp)='&quot;+context. vLoadDate+&quot;' &#13;&#10;) OWN1&#13;&#10;LEFT JOIN &#13;&#10;(SELECT &#9;OWN2.ActivationID AS ActivationID, OWN2.PreviousIdentityMasterId AS NextIdentityMaster FROM &#13;&#10;(SELECT &#13;&#10;&#9;ActivationID, PreviousIdentityMasterId,&#13;&#10;&#9;RANK() OVER(PARTITION BY ActivationID ORDER BY ChangeTimestamp ASC) as rank_own&#13;&#10;FROM &#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.cas_changeowner_ltd WHERE date(ChangeTimestamp)&gt;='&quot;+context. vLoadDate+&quot;'  &#13;&#10;) OWN2&#13;&#10;WHERE OWN2.rank_own=2) OWN3&#13;&#10;ON OWN1.ActivationID = OWN3.ActivationID&#13;&#10;--Getting sim changes&#13;&#10;UNION ALL&#9;&#13;&#10;SELECT &#13;&#10;&#9;ActivationID AS ActivationID, &#13;&#10;&#9;CreationDate  AS scd_start,&#13;&#10;&#9;'SIM CHANGE' AS row_change_reason,&#13;&#10;&#9;IMSI::varchar AS updated_value1,&#13;&#10;&#9;ICCID::varchar AS updated_value2&#13;&#10;FROM &quot;+context.connection_verticadb_schema_prod+&quot;.cas_simreplacement_ltd WHERE date(CreationDate) = '&quot;+context. vLoadDate+&quot;' &#13;&#10;ORDER BY  ActivationID&#13;&#10;) OUT1&#9;&#13;&#10;ON ACT.ActivationID = OUT1.ActivationID )OUT2&#13;&#10;--Getting subscription and status changes&#13;&#10;UNION ALL&#13;&#10;SELECT &#13;&#10;&#9;ACT2.ActivationID,&#13;&#10;&#9;SUB.NobillAccountNumber,&#13;&#10;&#9;SUB.scd_start,&#13;&#10;&#9;SUB.row_change_reason,&#13;&#10;&#9;SUB.updated_value1,&#13;&#10;&#9;SUB.updated_value2,&#13;&#10;&#9;SUB.updated_value3,&#13;&#10;&#9;SUB.rank_status&#13;&#10;FROM&#13;&#10;(SELECT &#13;&#10;&#9;ID AS ActivationID,&#13;&#10;&#9;NobillAccountNumber,&#13;&#10;&#9;MSISDN&#13;&#10;FROM &#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.cas_activations_ltd&#13;&#10;ORDER BY &#13;&#10;&#9;NobillAccountNumber, MSISDN ) ACT2&#13;&#10;INNER JOIN&#13;&#10;(SELECT &#13;&#10;&#9;b.accountnum AS NobillAccountNumber,&#13;&#10;&#9;localtstamp AS scd_start,&#13;&#10;&#9;CASE WHEN isSubtypeChangedFlag = 1 THEN 'SUBSCRIPTION CHANGE' &#13;&#10;&#9;          WHEN isBlockedStatusChangedFlag = 1 THEN 'BLOCKED STATUS CHANGE' ELSE 'SUBSCRIPTION STATUS CHANGE' END AS row_change_reason,&#13;&#10;&#9;CASE WHEN isSubtypeChangedFlag = 1 THEN new_subscription_type ELSE NULL END AS updated_value1,&#13;&#10;&#9;CASE WHEN isSubtypeChangedFlag = 0 THEN status::varchar ELSE NULL END AS updated_value2,&#13;&#10;&#9;CASE WHEN isBlockedStatusChangedFlag = 1 THEN status::varchar ELSE NULL END AS updated_value3,&#13;&#10;&#9;CASE WHEN status ='BEFORE FIRST USE' THEN 1 --These are not nobill_status numbers&#13;&#10;&#9;&#9; WHEN status ='IN USE' THEN 2&#13;&#10;&#9;&#9; WHEN status ='BLOCKED' THEN 3&#13;&#10;&#9;&#9; WHEN status ='COOLING' THEN 4&#13;&#10;&#9;&#9; WHEN status ='COLD' THEN 5&#13;&#10;&#9;&#9; WHEN status ='FROZEN' THEN 6&#13;&#10;&#9;&#9; WHEN status ='TERMINATED' THEN -1 ELSE 8 END AS rank_status,&#13;&#10; &#9;MSISDN&#13;&#10;FROM &#13;&#10;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.nobill_tx_subscription_history  a &#13;&#10;LEFT JOIN &#13;&#10;&#9;(select distinct subscriptionnum,e164,accountnum from &#9;&quot;+context.connection_verticadb_schema_stg+&quot;.nobill_tx_phonenumber_history where localtstamp::date = '&quot;+context. vLoadDate+&quot;' ) b &#13;&#10;on a.subscriptionnum = b.subscriptionnum and a.msisdn = b.e164 &#13;&#10;WHERE &#13;&#10;&#9;date(localtstamp) = '&quot;+context. vLoadDate+&quot;'&#13;&#10;&#9;AND b.accountnum IS NOT NULL&#13;&#10;&#9;AND (isSubtypeChangedFlag = 1 or isBlockedStatusChangedFlag = 1 or status is not NULL)&#13;&#10;ORDER BY &#13;&#10; &#9;b.accountnum, MSISDN&#13;&#10;) SUB&#13;&#10;ON &#13;&#10;&#9;SUB.NobillAccountNumber = ACT2.NobillAccountNumber &#13;&#10;&#9;AND SUB.MSISDN = ACT2.MSISDN&#13;&#10;ORDER BY scd_start;&#13;&#10;&#13;&#10;--drop and create table with ranks to avoid repeated RANK() function call&#13;&#10;DROP TABLE IF EXISTS &quot;+context.connection_verticadb_schema_stg+&quot;.nobill_dim_customer_candidates_rank CASCADE;&#13;&#10;&#13;&#10;CREATE TABLE &#13;&#10;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.nobill_dim_customer_candidates_rank AS &#13;&#10;SELECT  *,&#13;&#10;&#9;&#9;RANK() OVER(PARTITION BY ActivationID, NobillAccountNumber ORDER BY scd_start ASC, rank_status DESC) AS rank_cand&#13;&#10;FROM &#13;&#10;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.nobill_dim_customer_candidates ;&#13;&#10;&#13;&#10;SELECT ANALYZE_STATISTICS('&quot;+context.connection_verticadb_schema_stg+&quot;.nobill_dim_customer_candidates_rank');&#13;&#10;SELECT ANALYZE_STATISTICS('&quot;+context.connection_verticadb_schema_stg+&quot;.cas_activations_ltd_updated');&quot;"/>
