    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;INSERT INTO &quot;+context.connection_verticadb_schema_prod+&quot;.nobill_dim_customer_scd&#13;&#10;SELECT&#13;&#10;&#9;OUT2.activationID,&#13;&#10;&#9;OUT2.accountnum,&#13;&#10;&#9;OUT2.msisdn,&#13;&#10;&#9;OUT2.imsi,&#13;&#10;&#9;OUT2.subscriptiontype,&#13;&#10;&#9;OUT2.nobill_status,&#13;&#10;&#9;OUT2.block_status,&#13;&#10;&#9;OUT2.activation_date,&#13;&#10;&#9;OUT2.termination_date,&#13;&#10;&#9;OUT2.customer_id_type,&#13;&#10;&#9;CID.customer_id_number,&#13;&#10;&#9;CID.customer_firstname,&#13;&#10;&#9;CID.customer_lastname,&#13;&#10;&#9;CID.customer_border_number,&#13;&#10;&#9;OUT2.IdentityMasterId,&#13;&#10;&#9;OUT2.nationality,&#13;&#10;&#9;OUT2.is_portIn_activation_flag,&#13;&#10;&#9;OUT2.is_eSim_activation_flag,&#13;&#10;&#9;OUT2.is_portedOut_flag,&#13;&#10;&#9;OUT2.iccid,&#13;&#10;&#9;OUT2.puk,&#13;&#10;&#9;OUT2.row_change_reason,&#13;&#10;&#9;OUT2.start_date, &#13;&#10;&#9;case when OUT2.termination_date is not NULL then OUT2.termination_date else '9999-01-01 12:00:00' end AS scd_end,&#13;&#10;&#9;1 AS scd_active&#13;&#10;FROM&#13;&#10;(&#13;&#10;SELECT&#13;&#10;&#9;CAS.activationID AS activationID,&#13;&#10;&#9;CAS.accountnum AS accountnum,&#13;&#10;&#9;CAS.msisdn AS msisdn,&#13;&#10;&#9;CASE WHEN CAND.imsi IS NULL THEN ACT2.imsi ELSE CAND.imsi END AS imsi,&#13;&#10;&#9;CASE WHEN CAND.subscriptiontype IS NULL THEN ACT2.subscriptiontype ELSE CAND.subscriptiontype END AS subscriptiontype,&#13;&#10;&#9;CASE WHEN CAND.nobill_status IS NULL THEN ACT2.nobill_status ELSE CAND.nobill_status END AS nobill_status,&#13;&#10;&#9;CASE WHEN CAND.block_status IS NULL THEN ACT2.block_status ELSE CAND.block_status END AS block_status,&#13;&#10;&#9;ACT2.activation_date AS activation_date,&#13;&#10;&#9;CASE WHEN CAND.termination_date IS NOT NULL THEN CAND.termination_date &#13;&#10;&#9;&#9;&#9;   WHEN ACT2.termination_date IS NOT NULL THEN ACT2.termination_date ELSE NULL END AS termination_date,&#13;&#10;&#9;CAS.customer_id_type AS customer_id_type,&#13;&#10;&#9;CASE WHEN CAND.IdentityMasterId::int IS NOT NULL THEN CAND.IdentityMasterId::int ELSE ACT2.IdentityMasterId::int END AS IdentityMasterId,&#13;&#10;&#9;CAS.nationality AS nationality,&#13;&#10;&#9;CAS.is_portIn_activation_flag AS is_portIn_activation_flag,&#13;&#10;&#9;CAS.is_eSim_activation_flag AS is_eSim_activation_flag,&#13;&#10;&#9;CAS.is_portedOut_flag AS is_portedOut_flag,&#13;&#10;&#9;ACT2.iccid AS iccid,&#13;&#10;&#9;CAS.puk AS puk,&#13;&#10;&#9;CAND.row_change_reason AS row_change_reason,&#13;&#10;&#9;CAND.scd_start AS start_date&#13;&#10;FROM&#13;&#10;(SELECT&#13;&#10;    Id AS activationID,&#13;&#10;&#9;NobillAccountNumber AS accountnum,&#13;&#10;&#9;MSISDN AS msisdn,&#13;&#10;&#9;ID_Type AS customer_id_type,&#13;&#10;&#9;ID_Nationality AS nationality,&#13;&#10;&#9;IsPortInActivation AS is_portIn_activation_flag,&#13;&#10;&#9;IsEsimActivation AS is_eSim_activation_flag,&#13;&#10;&#9;IsPortedOut AS is_portedOut_flag,&#13;&#10;&#9;PUK AS puk,&#13;&#10;&#9;IdentityMasterId&#13;&#10; FROM&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.cas_activations_ltd &#13;&#10; WHERE &#13;&#10;&#9;Id IN (SELECT ActivationID FROM &quot;+context.connection_verticadb_schema_stg+&quot;.nobill_dim_customer_candidates_rank WHERE rank_cand=&quot;+((Integer)globalMap.get(&quot;tLoop_2_CURRENT_VALUE&quot;))+&quot;)&#13;&#10; ORDER BY &#13;&#10;&#9;Id&#9;) CAS&#9;&#13;&#10;INNER JOIN &#13;&#10;(SELECT &#13;&#10;&#9;ActivationID,&#13;&#10;&#9;NobillAccountNumber,&#13;&#10;&#9;scd_start AS scd_start,&#13;&#10;&#9;row_change_reason AS row_change_reason,&#13;&#10;&#9;CASE WHEN row_change_reason='SIM CHANGE' THEN updated_value1::int ELSE NULL END AS imsi,&#13;&#10;&#9;CASE WHEN row_change_reason='SIM CHANGE' THEN updated_value2::int ELSE NULL END AS iccid,&#13;&#10;&#9;CASE WHEN row_change_reason='SUBSCRIPTION CHANGE' THEN updated_value1 ELSE NULL END AS subscriptiontype,&#13;&#10;&#9;CASE WHEN row_change_reason='SUBSCRIPTION STATUS CHANGE' THEN updated_value2 ELSE NULL END AS nobill_status,&#13;&#10;&#9;CASE WHEN row_change_reason='BLOCKED STATUS CHANGE' THEN updated_value3 ELSE NULL END AS block_status,&#13;&#10;    CASE WHEN row_change_reason='SUBSCRIPTION STATUS CHANGE' AND  updated_value2 = 'TERMINATED' THEN scd_start ELSE NULL END AS termination_date,&#13;&#10;&#9;CASE WHEN row_change_reason IN ('ID CHANGE','OWNER CHANGE') THEN updated_value1 ELSE NULL END AS IdentityMasterId&#13;&#10; FROM&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.nobill_dim_customer_candidates_rank OUT1&#9;&#9;&#13;&#10; WHERE OUT1.rank_cand=&quot;+((Integer)globalMap.get(&quot;tLoop_2_CURRENT_VALUE&quot;))+&quot;&#13;&#10; ORDER BY &#13;&#10;&#9;ActivationID) CAND&#9;&#9;&#13;&#10;ON &#13;&#10;&#9;CAS.activationID = CAND.ActivationID&#13;&#10;INNER JOIN &#13;&#10;(SELECT &#13;&#10;&#9;Id AS activationID,&#13;&#10;&#9;NobillAccountNumber,&#13;&#10;&#9;ActivationDate AS activation_date,&#13;&#10;&#9;TerminationDate AS termination_date,&#13;&#10;&#9;imsi,&#13;&#10;&#9;iccid,&#13;&#10;&#9;NobillSubscriptionType AS subscriptiontype,&#13;&#10;&#9;nobill_status,&#13;&#10;&#9;block_status,&#13;&#10;&#9;IdentityMasterId&#13;&#10; FROM&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.cas_activations_ltd_updated &#13;&#10; ORDER BY &#13;&#10;&#9;ActivationID) ACT2&#9;&#13;&#10;ON &#13;&#10;&#9;ACT2.activationID = CAS.activationID &#13;&#10;ORDER BY &#13;&#10;&#9;IdentityMasterId) OUT2&#13;&#10;INNER JOIN&#13;&#10;(SELECT&#13;&#10;&#9;Id,&#13;&#10;&#9;IdNumber AS customer_id_number,&#13;&#10;&#9;FirstName AS customer_firstname,&#13;&#10;&#9;LastName AS customer_lastname,&#13;&#10;&#9;BorderNumber AS customer_border_number&#13;&#10; FROM&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.cas_id_master_ltd CID &#13;&#10; ORDER BY &#13;&#10;&#9;Id) CID&#9;&#13;&#10;ON &#13;&#10;&#9;CID.Id&#9;= OUT2.IdentityMasterId&#13;&#10;ORDER BY 1,2,22&#9;&quot;"/