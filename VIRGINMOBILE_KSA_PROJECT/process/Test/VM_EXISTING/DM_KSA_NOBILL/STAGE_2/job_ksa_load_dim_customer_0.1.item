ionID, PreviousIdentityMasterId, RANK() OVER(PARTITION BY ActivationID ORDER BY ChangeTimestamp DESC) AS rank_own &#13;&#10;&#9;  FROM &quot;+context.connection_verticadb_schema_prod+&quot;.cas_changeowner_ltd WHERE date(ChangeTimestamp) &lt;='&quot;+context.vLoadDate+&quot;' ) b &#13;&#10;WHERE a.ID = b.ActivationID&#13;&#10; &#9;  AND b.rank_own=1&#13;&#10; &#9;  AND date(a.ActivationDate) = '&quot;+context.vLoadDate+&quot;';&#13;&#10;&#13;&#10;--Update Unchanged IdentityMasterId&#13;&#10;UPDATE &quot;+context.connection_verticadb_schema_stg+&quot;.cas_activations_ltd_updated AS a  &#13;&#10;SET IdentityMasterId = b.IdentityMasterId &#13;&#10;FROM (SELECT ID,NobillAccountNumber, IdentityMasterId FROM &quot;+context.connection_verticadb_schema_prod+&quot;.cas_activations_ltd WHERE date(CreatedOn) ='&quot;+context.vLoadDate+&quot;') b &#13;&#10;WHERE a.ID = b.ID&#9;  &#13;&#10;AND a.NobillAccountNumber = b.NobillAccountNumber&#13;&#10;AND a.IdentityMasterId IS NULL&#13;&#10;AND date(a.ActivationDate) = '&quot;+context.vLoadDate+&quot;';&quot;"/>
