    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;insert into prod.tmp_cvm_feed_01 &#13;&#10;select &#13;&#10;&#9;ad_date, &#13;&#10;&#9;to_char(ad_date,'YYYYMM') as date_yyyymm,&#13;&#10;&#9;ad_accountnum, &#13;&#10;&#9;sum(ad_total_received_revenue_payg + ad_total_received_revenue_upfront_bundle) as total_upfront_received_revenue ,&#13;&#10;&#9;SUM(CASE WHEN service_subtype &lt;&gt; 'Expired Credit' THEN ad_total_gross_revenue ELSE 0 END) uds_total_revenue_gross_traffic,&#13;&#10;&#9;SUM(CASE WHEN service_group = 'Voice' AND ad_call_direction_id &lt;&gt; 3 AND ad_terminationreason IN (0, 6, 7, 8, 10) THEN ad_total_duration / 60000 ELSE 0 END) uds_total_mins_voice_ex_cf,&#13;&#10;&#9;SUM(CASE WHEN service_group = 'SMS' THEN ad_total_revised_sms_count ELSE 0 END) AS uds_total_sms,&#13;&#10;&#9;SUM(CASE WHEN service_group = 'Data' THEN ad_total_revised_datavolume / 1024 / 1024 ELSE 0 END) AS uds_total_volume_data,&#13;&#10;&#9;SUM(CASE WHEN service_group = 'Recharge' THEN ad_total_adjustment_amount ELSE 0 END) AS uds_total_amount_recharge,&#13;&#10;&#9;SUM(CASE WHEN service_group = 'Voice' AND service_type = 'Voice Bundle' THEN ad_total_gross_revenue_upfront_bundle ELSE 0 END) uds_total_revenue_gross_voice_upfront_bundle,&#9;&#9;&#13;&#10;&#9;SUM(CASE WHEN service_group = 'Data'  AND service_type = 'Data Bundle'  THEN ad_total_gross_revenue_upfront_bundle ELSE 0 END) uds_total_revenue_gross_data_upfront_bundle&#13;&#10;from &#13;&#10;prod.nobill_aggr_ws_cost_V2 a &#13;&#10;left join &#13;&#10;prod.nobill_dim_transaction_key b&#13;&#10;on a.ad_transaction_id = b.transaction_id &#13;&#10;where ad_date &gt;= DATE('&quot;+context.vLoadDate+&quot;') - 120&#13;&#10;group by 1,2,3;&quot;"/>
