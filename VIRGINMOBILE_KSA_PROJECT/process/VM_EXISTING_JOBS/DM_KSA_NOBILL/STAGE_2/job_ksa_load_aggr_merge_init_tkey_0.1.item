_reset,&#13;&#10;&#9;a.ad_total_revenue_gross,&#13;&#10;&#9;a.ad_total_revenue_received,&#13;&#10;--initial balance&#13;&#10;&#9;ZEROIFNULL(CASE WHEN  dcr_category_2 = 'Initial Balance' THEN ad_total_revenue_gross ELSE 0 END)  AS ad_total_balance_initial,&#13;&#10;--voice&#13;&#10; CASE &#13;&#10;&#9;WHEN ad_service_type_id = 1 AND ad_call_direction_id = 2 AND doz_category_1 &lt;&gt; 'Roaming' AND dtz_category_1 = 'National' THEN 'MOC_Nat_' || dtz_category_2 || '_' || dtz_category_4&#13;&#10;&#9;WHEN ad_service_type_id = 1 AND ad_call_direction_id = 2 AND doz_category_1 &lt;&gt; 'Roaming' AND dtz_category_1 = 'International' THEN 'MOC_Intl_' || dtz_Category_3 &#13;&#10;&#9;WHEN ad_service_type_id = 1 AND ad_call_direction_id = 3 AND dtz_category_1 = 'National' THEN 'CF_Nat_' || dtz_category_4&#13;&#10;&#9;WHEN ad_service_type_id = 1 AND ad_call_direction_id = 2 AND doz_category_1 = 'Roaming' AND doz_category_2 = dtz_category_3 THEN 'MOC_Roam_' || doz_category_2 || '_Local'&#13;&#10;&#9;WHEN ad_service_type_id = 1 AND ad_call_direction_id = 2 AND doz_category_1 = 'Roaming' AND dtz_category_3 = 'Saudi Arabia' THEN 'MOC_Roam_' || doz_category_2 || '_Home'&#13;&#10;&#9;WHEN ad_service_type_id = 1 AND ad_call_direction_id = 2 AND doz_category_1 = 'Roaming' AND doz_category_2 &lt;&gt; dtz_category_3 AND dtz_category_3 &lt;&gt; 'Saudi Arabia' THEN 'MOC_Roam_' || doz_category_2 || '_Other'&#13;&#10;&#9;WHEN ad_service_type_id = 1 AND ad_call_direction_id = 1 AND dtz_category_1 &lt;&gt; 'Roaming' THEN 'MTC_' || dcop_category_2 || '_' || dcop_category_4&#13;&#10;&#9;WHEN ad_service_type_id = 1 AND ad_call_direction_id = 1 AND dtz_category_1 = 'Roaming'  THEN  'MTC_Roam_' || dtz_category_3 &#13;&#10;--SMS&#13;&#10;&#9;WHEN ad_service_type_id = 2  AND doz_category_3 = 'Local'  AND dtz_category_1 = 'National' THEN 'SMS_Nat_' || dtz_category_4&#13;&#10;    WHEN ad_service_type_id = 2  AND doz_category_3 = 'Local'  AND dtz_category_1 = 'International' THEN 'SMS_Intl_' || dtz_category_3&#13;&#10;&#9;WHEN ad_service_type_id = 2  AND doz_category_1 = 'Roaming' THEN 'SMS_Roam_' || doz_category_3&#13;&#10;--Data&#13;&#10;&#9;WHEN ad_service_type_id = 5 AND doz_category_1 = 'Local' THEN 'Data_National'&#13;&#10;&#9;WHEN ad_service_type_id = 5 AND doz_category_1 = 'Roaming' THEN 'Data_Roam_'|| doz_category_2&#13;&#10;--Event&#13;&#10;&#9;WHEN ad_service_type_id = 13 AND dev_category_2 = 'International Credit Transfer' THEN 'Event_VAS_International_Credit_Transfer'&#13;&#10;&#9;WHEN ad_service_type_id = 13 AND dev_category_2 &lt;&gt; 'International Credit Transfer' THEN 'Event_'|| dev_category_3&#13;&#10;--Recharge&#13;&#10;&#9;WHEN dad_category_1 = 'Recharge' AND dad_category_2 = 'Voucher' THEN 'Recharge_Voucher'&#13;&#10;&#9;WHEN dad_category_1 = 'Recharge' AND dad_category_2 = 'eLoad' THEN 'Recharge_eLoad'&#13;&#10;&#9;WHEN dad_category_1 = 'Recharge' AND dad_category_2 = 'Credit Card' THEN 'Recharge_Credit_Card'&#13;&#10;&#9;WHEN dad_category_1 = 'Recharge' AND dad_category_2 = 'BCRM/Sadad' THEN 'Recharge_BCRM_Sadad'&#13;&#10;&#9;WHEN dad_category_1 = 'Recharge' AND dad_category_2 = 'Pay IN Store' THEN 'Recharge_Pay_In_Store'&#13;&#10;--Adjustment&#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND dad_category_2 = 'Starterpack' THEN 'Adjustment_Starterpack'&#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND dad_category_2 = 'Voucher' THEN 'Adjustment_Voucher'&#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND dad_category_2 = 'eLoad' THEN 'Adjustment_eLoad'&#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND dad_category_2 = 'Credit Card' THEN 'Adjustment_Credit_Card'&#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND dad_category_2 = 'BCRM/Sadad' THEN 'Adjustment_BCRM_Sadad' &#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND dad_category_2 = 'Pay IN Store' THEN 'Adjustment_Pay_In_Store' &#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND dad_adjustmentreasonname = 'Overdraw' THEN 'Overdraw'&#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND dad_category_3 = 'Account_Fund_Withdraw' THEN 'Account_Fund_Withdraw'&#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND ad_source = 1 AND dad_category_3 = 'Commission_Payout' THEN 'Commission_Payout'&#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND ad_source = 1 AND dad_category_3 = 'Commission_Redemption' THEN 'Commission_Redemption'&#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND ad_source = 1 AND dad_category_3 = 'Manual' THEN 'Manual_Credit_Adjustment'&#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND ad_source = 1 AND dad_category_3 = 'Migration' THEN 'Migration'&#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND ad_source = 1 AND dad_category_3 = 'Migration_Fee' THEN 'Migration_Fee'&#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND ad_source = 1 AND dad_category_3 = 'Postpaid_Payment' THEN 'Postpaid_Payment'&#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND ad_source = 1 AND dad_category_2 = 'VAT' THEN 'Adjustment_VAT'&#13;&#10;--Expired &#13;&#10;&#9;WHEN dad_category_1 = 'Adjustment' AND ad_source = 1 AND dad_category_3 = 'Expired Balance' THEN 'Expired_Credit'&#9;&#9;&#13;&#10;&#9;WHEN ad_source = 0  AND ad_updatetype IN (4,6) AND dcr_category_7 = 'Amount' AND dcr_category_6 = 'Consuming' THEN 'Expired_Credit'&#13;&#10;    WHEN ad_source = 0  AND ad_updatetype IN (4,6) AND dcr_category_7 = 'Time' AND dcr_category_6 = 'Consuming' THEN 'Expired_Voice'&#13;&#10;&#9;WHEN ad_source = 0  AND ad_updatetype IN (4,6) AND dcr_category_7 = 'Data' AND dcr_category_6 = 'Consuming' THEN 'Expired_Data'&#13;&#10;--Others&#13;&#10;&#9;ELSE ad_source || '_' || ad_service_type_id || '_Undefined' END AS ad_transaction_key,&#13;&#10;--Revised Volumes &#13;&#10;&#9;((ad_total_duration_counter)/60000 + (ad_total_data_volume_counter)/1024^2) AS ad_total_counter_volume_revised,&#13;&#10;&#9;((ad_total_duration_counter_expired)/60000 + (ad_total_data_volume_expired)/1024^2) AS ad_total_counter_volume_revised_expired,&#13;&#10;&#9;((ad_total_duration_counter_reset)/60000 + (ad_total_data_volume_reset)/1024^2) AS ad_total_counter_volume_revised_reset,&#13;&#10;CASE WHEN ad_service_type_id = 1 AND ad_terminationreason IN (0,6,7,8,10) THEN ad_total_duration_actual/60000 &#13;&#10;     WHEN ad_service_type_id = 2 THEN ad_total_quantity &#13;&#10;     WHEN ad_service_type_id = 5 THEN ad_total_data_volume_actual/1024^2 ELSE 0 END AS ad_total_volume_revised,&#13;&#10;--payg_excluding_upfront_bundle_subscriptions&#13;&#10;&#9;ZEROIFNULL(ad_total_revenue_gross) - (CASE WHEN ad_service_type_id = 13 AND dev_report_type = 'upfront_subscription' THEN ZEROIFNULL(ad_total_revenue_gross) ELSE 0 END ) AS ad_total_revenue_gross_payg,&#13;&#10;&#9;ZEROIFNULL(ad_total_revenue_received) - (CASE WHEN ad_service_type_id = 13 AND dev_report_type = 'upfront_subscription' THEN ZEROIFNULL(ad_total_revenue_received) ELSE 0 END) AS ad_total_revenue_received_payg,&#13;&#10;-- upfront bundle subscriptions&#13;&#10;CASE WHEN ad_service_type_id = 13 AND dev_report_type = 'upfront_subscription' THEN ZEROIFNULL(ad_total_revenue_gross) ELSE 0 END AS ad_total_revenue_gross_upfront_bundle,&#13;&#10;CASE WHEN ad_service_type_id = 13 AND dev_report_type = 'upfront_subscription' THEN ZEROIFNULL(ad_total_revenue_received) ELSE 0 END AS ad_total_revenue_received_upfront_bundle,&#13;&#10;&#9;0 AS ad_total_revenue_gross_time_bundle,&#13;&#10;&#9;0 AS ad_total_revenue_received_time_bundle,&#13;&#10; CASE WHEN ad_service_type_id = 1 AND ad_terminationreason IN (0,6,7,8,10)  AND ad_call_direction_id = 2 AND doz_category_1 = 'Roaming' THEN  ad_total_duration_min&#13;&#10;             WHEN ad_service_type_id = 1 AND ad_terminationreason IN (0,6,7,8,10)  AND ad_call_direction_id = 1 AND dtz_category_1 = 'Roaming'  THEN  ad_total_duration_min&#13;&#10;&#9;&#9;&#9; WHEN ad_service_type_id = 1 AND ad_terminationreason IN (0,6,7,8,10)  THEN ad_total_duration_sec/60&#13;&#10;&#9;&#9;&#9; WHEN ad_service_type_id = 2 THEN ad_total_quantity&#13;&#10;&#9;&#9;&#9; WHEN ad_service_type_id = 5 THEN ad_total_data_volume_actual/1000^2 ELSE 0 END AS ad_total_volume_wholesale,&#13;&#10;&#9;&#9;&#9; a.ad_package_id&#13;&#10;FROM &#13;&#10;&#9;&quot;+context.connection_verticadb_schema_stg+&quot;.nobill_aggr_merge_init a &#13;&#10;LEFT JOIN (select&#13;&#10;&#9;dev_eventid_base as dev_event_id,&#13;&#10;&#9;max(dev_report_type) dev_report_type,&#13;&#10;&#9;max(dev_category_1) dev_category_1,&#13;&#10;&#9;max(dev_category_2) dev_category_2,&#13;&#10;&#9;max(dev_category_3) dev_category_3,&#13;&#10;&#9;max(dev_category_4) dev_category_4&#13;&#10;from&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.nobill_dim_event&#13;&#10;group by&#13;&#10;&#9;1&#13;&#10;order by&#13;&#10;&#9;1)  b ON a.ad_eventid = b.dev_event_id&#13;&#10;LEFT JOIN &quot;+context.connection_verticadb_schema_prod+&quot;.nobill_dim_origzone c ON c.doz_origzone_id = a.ad_origzone_id&#13;&#10;LEFT JOIN &quot;+context.connection_verticadb_schema_prod+&quot;.nobill_dim_termzone d ON d.dtz_termzone_id = a.ad_termzone_id&#13;&#10;LEFT JOIN &quot;+context.connection_verticadb_schema_prod+&quot;.nobill_dim_country_operator e ON e.dcop_country_operator_id = a.ad_country_operator_id&#13;&#10;LEFT JOIN &quot;+context.connection_verticadb_schema_prod+&quot;.nobill_dim_adjustment_type f ON f.dad_adjustment_type_id = a.ad_adjustment_type_id&#13;&#10;LEFT JOIN &quot;+context.connection_verticadb_schema_prod+&quot;.nobill_dim_counter_type g ON g.dcr_counter_type_id = a.ad_counter_type_id&#13;&#10;WHERE &#13;&#10;ad_date =  '&quot;+context.vLoadDate+&quot;'&#13;&#10;&quot;"/>
