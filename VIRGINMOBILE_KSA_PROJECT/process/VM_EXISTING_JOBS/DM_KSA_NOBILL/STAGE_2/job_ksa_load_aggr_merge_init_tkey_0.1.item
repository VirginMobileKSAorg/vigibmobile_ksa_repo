    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;INSERT INTO &quot;+context.connection_verticadb_schema_stg+&quot;.nobill_aggr_merge_init&#13;&#10;SELECT &#13;&#10;&#9;0 AS ad_source,&#13;&#10;&#9;facc_date AS ad_date,&#13;&#10;&#9;--ad_hour column for Oman&#13;&#10;&#9;NULL AS ad_hour,&#13;&#10;&#9;--ad_subscription_date is populated IN tkey FROM subscriptin rate AND fact event subscription table&#13;&#10;&#9;NULL AS ad_subscription_date,&#13;&#10;&#9;fus_subscription_type_id AS ad_subscription_type_id,&#13;&#10;&#9;fus_accountnum AS ad_accountnum,&#13;&#10;&#9;facc_msisdn AS ad_msisdn,&#13;&#10;&#9;fus_imsi AS ad_imsi,&#13;&#10;&#9;fus_call_direction_id AS ad_call_direction_id,&#13;&#10;&#9;fus_calltype AS ad_calltype,&#13;&#10;&#9;fus_terminationreason AS ad_terminationreason,&#13;&#10;&#9;fus_mobilecountrycode AS ad_mobilecountrycode,&#13;&#10;&#9;fus_mobilenetworkcode AS ad_mobilenetworkcode,&#13;&#10;&#9;fus_cell_site_id AS ad_cell_site_id,&#13;&#10;&#9;fus_service_type_id AS ad_service_type_id,&#13;&#10;&#9;fus_country_operator_id AS ad_country_operator_id,&#13;&#10;&#9;--ad_eris_country_operator_id column for Oman&#13;&#10;&#9;NULL AS ad_eris_country_operator_id,&#13;&#10;&#9;fus_origzone_id AS ad_origzone_id,&#13;&#10;&#9;fus_termzone_id AS ad_termzone_id,&#13;&#10;    CASE WHEN dcr_category_1 IS NOT NULL AND FACC.dcr_category_1 = 'Monetary' THEN FUS.dev_eventid_base ELSE FACC.dcr_category_4::INT END AS ad_eventid,&#13;&#10;&#9;0 AS ad_adjustment_type_id,&#13;&#10;&#9;0 AS ad_voucher_type_id,&#13;&#10;&#9;facc_counter_type_id AS ad_counter_type_id,&#13;&#10;&#9;facc_counterid AS ad_counter_id,&#13;&#10;&#9;facc_updatetype AS ad_updatetype,&#13;&#10;&#9;0 AS ad_quantity,&#13;&#10;&#9;0 AS ad_total_adjustment_amount,&#13;&#10;&#9;SUM(ZEROIFNULL(CASE WHEN facc_updatetype = 2 &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;THEN facc_valuediff &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;ELSE 0 END)) AS ad_total_valuediff,&#13;&#10;&#9;0 AS ad_total_duration_actual,&#13;&#10;    0 AS ad_total_duration_sec,&#13;&#10;    0 AS ad_total_duration_min,&#13;&#10;&#9;0 AS ad_total_duration_charged,&#13;&#10;&#9;0 AS ad_total_duration,&#13;&#10;&#9;0 AS ad_total_duration_payg,&#13;&#10;&#9;ABS(SUM(CASE WHEN dcr_category_1 IS NOT NULL AND dcr_category_1 = 'Voice' AND facc_updatetype = 2 &#13;&#10;&#9;&#9;&#9;     THEN facc_valuediff::numeric(25,6)&#13;&#10;&#9;&#9;&#9;&#9; ELSE 0 END)) AS ad_total_duration_counter,&#13;&#10;&#9;0 AS ad_total_duration_counter_expired,&#13;&#10;&#9;0 AS ad_total_duration_counter_reset,&#13;&#10;&#9;0 AS ad_total_sms,&#13;&#10;&#9;0 AS ad_total_sms_payg,&#13;&#10;&#9;ABS(SUM(CASE WHEN dcr_category_1 IS NOT NULL AND dcr_category_1 = 'SMS' AND facc_updatetype = 2 &#13;&#10;&#9;&#9;&#9;&#9; THEN facc_valuediff::numeric(25,6)&#13;&#10;&#9;&#9;&#9;&#9; ELSE 0 END)) AS ad_total_sms_counter,&#13;&#10;&#9;0 AS ad_total_sms_counter_expired,&#13;&#10;&#9;0 AS ad_total_sms_counter_reset,&#13;&#10;&#9;0 AS ad_total_data_volume_received,&#13;&#10;&#9;0 AS ad_total_data_volume_sent,&#13;&#10;&#9;0 AS ad_total_data_volume_actual,&#13;&#10;&#9;0 AS ad_total_data_volume,&#13;&#10;&#9;0 AS ad_total_data_volume_payg,&#13;&#10;&#9;ABS(SUM(CASE WHEN dcr_category_1 IS NOT NULL AND dcr_category_1 = 'Data' AND facc_updatetype = 2 &#13;&#10;&#9;&#9;&#9;&#9; THEN facc_valuediff::numeric(25,6)&#13;&#10;&#9;&#9;&#9;&#9; ELSE 0 END)) AS ad_total_data_volume_counter,&#13;&#10;&#9;0 AS ad_total_data_volume_expired,&#13;&#10;&#9;0 AS ad_total_data_volume_reset,&#13;&#10;&#9;ABS(SUM(CASE WHEN dcr_category_1 IS NOT NULL AND dcr_category_1 = 'Monetary' AND facc_updatetype = 2 &#13;&#10;&#9;&#9;&#9;&#9; THEN facc_valuediff &#13;&#10;&#9;&#9;&#9;&#9; ELSE 0 END)) AS ad_total_revenue_gross,&#13;&#10;&#9;(ABS(SUM(CASE WHEN dcr_category_1 IS NOT NULL AND dcr_category_1 = 'Monetary' AND facc_updatetype = 2 &#13;&#10;&#9;&#9;&#9;&#9; THEN facc_valuediff &#13;&#10;&#9;&#9;&#9;&#9; ELSE 0 END)) + &#13;&#10;&#9;(SUM(CASE WHEN dcr_category_2 IS NOT NULL AND dcr_category_2 = 'Monetary Bonus' AND facc_updatetype = 2 &#13;&#10;&#9;&#9;&#9;  THEN facc_valuediff &#13;&#10;&#9;&#9;&#9;  ELSE 0 END))) AS ad_total_revenue_received,&#13;&#10;&#9;--ad_dpo_id column for Oman peak off-peak&#13;&#10;&#9;NULL AS ad_dpo_id,&#13;&#10;&#9;    CASE WHEN dcr_category_1 IS NOT NULL AND FACC.dcr_category_1 = 'Monetary' THEN FUS.fus_additionalinfo2 ELSE null  END AS ad_package_id&#13;&#10;FROM&#13;&#10;(&#13;&#10;(SELECT&#13;&#10;&#9;facc_date,&#13;&#10;&#9;facc_msisdn,&#13;&#10;&#9;facc_counter_type_id,&#13;&#10;&#9;facc_counterid,&#13;&#10;&#9;facc_updatetype,&#13;&#10;&#9;ZEROIFNULL(facc_valuediff)  AS facc_valuediff,&#13;&#10;&#9;facc_parentreferenceid,&#13;&#10;&#9;dcr_category_1,&#13;&#10;&#9;dcr_category_4,&#13;&#10;&#9;dcr_category_2&#13;&#10;FROM&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.nobill_fact_account_counter&#13;&#10;INNER JOIN &#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.nobill_dim_counter_type dcr &#13;&#10;ON &#13;&#10;&#9;facc_counter_type_id = dcr.dcr_counter_type_id &#13;&#10;WHERE&#13;&#10;&#9;facc_date = '&quot;+context.vLoadDate+&quot;'&#13;&#10;&#9;AND facc_updatetype = 2 &#13;&#10;ORDER BY &#13;&#10;&#9;facc_parentreferenceid ) FACC &#13;&#10;INNER JOIN &#13;&#10;(SELECT &#13;&#10;    fus_internalreferenceid,&#13;&#10;&#9;fus_subscription_type_id ,&#13;&#10;&#9;fus_accountnum ,&#13;&#10;&#9;fus_imsi,&#13;&#10;&#9;dev_eventid_base,&#13;&#10;&#9;fus_call_direction_id,&#13;&#10;&#9;fus_calltype,&#13;&#10;&#9;fus_terminationreason,&#13;&#10;&#9;fus_mobilecountrycode,&#13;&#10;&#9;fus_mobilenetworkcode,&#13;&#10;&#9;fus_cell_site_id,&#13;&#10;&#9;fus_service_type_id,&#13;&#10;&#9;fus_country_operator_id,&#13;&#10;&#9;fus_origzone_id,&#13;&#10;&#9;fus_termzone_id,&#13;&#10;&#9;fus_additionalinfo2&#13;&#10;FROM&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.nobill_fact_usage a&#13;&#10;&#9;left join &quot;+context.connection_verticadb_schema_prod+&quot;.nobill_dim_event b&#13;&#10;&#9; on a.fus_event_id=b.dev_event_id&#13;&#10;WHERE&#13;&#10;&#9;fus_date = '&quot;+context.vLoadDate+&quot;'&#13;&#10;    &#13;&#10;ORDER BY&#13;&#10;&#9;fus_internalreferenceid ) FUS &#13;&#10;ON &#13;&#10;  FACC.facc_parentreferenceid = FUS.fus_internalreferenceid&#13;&#10;)&#13;&#10;GROUP BY &#13;&#10;&#9;facc_date,&#13;&#10;&#9;fus_subscription_type_id,&#13;&#10;&#9;fus_accountnum,&#13;&#10;&#9;facc_msisdn,&#13;&#10;&#9;fus_imsi,&#13;&#10;&#9;fus_call_direction_id,&#13;&#10;&#9;fus_calltype,&#13;&#10;&#9;fus_terminationreason,&#13;&#10;&#9;fus_mobilecountrycode,&#13;&#10;&#9;fus_mobilenetworkcode,&#13;&#10;&#9;fus_cell_site_id,&#13;&#10;&#9;fus_service_type_id,&#13;&#10;&#9;fus_country_operator_id,&#13;&#10;&#9;fus_origzone_id,&#13;&#10;&#9;fus_termzone_id,&#13;&#10;&#9;CASE WHEN dcr_category_1 IS NOT NULL AND dcr_category_1 = 'Monetary' THEN dev_eventid_base ELSE dcr_category_4::INT END,&#13;&#10;&#9;facc_counter_type_id,&#13;&#10;&#9;facc_counterid,&#13;&#10;&#9;facc_updatetype,&#13;&#10;&#9;CASE WHEN dcr_category_1 IS NOT NULL AND FACC.dcr_category_1 = 'Monetary' THEN FUS.fus_additionalinfo2 ELSE null  END&#13;&#10;;&quot;&#13;&#10;&#13;&#10;"/>
