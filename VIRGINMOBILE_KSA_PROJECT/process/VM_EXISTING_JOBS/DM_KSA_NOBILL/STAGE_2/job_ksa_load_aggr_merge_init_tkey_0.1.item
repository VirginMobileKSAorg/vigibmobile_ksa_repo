    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;INSERT INTO &quot;+context.connection_verticadb_schema_stg+&quot;.nobill_aggr_merge_init&#13;&#10;SELECT&#13;&#10;&#9;1 AS ad_source,&#13;&#10;    fus_date AS ad_date,&#13;&#10;&#9;--ad_hour column for Oman&#13;&#10;&#9;NULL AS ad_hour,&#13;&#10;&#9;--ad_subscription_date is populated IN tkey FROM subscriptin rate AND fact event subscription table&#13;&#10;&#9;NULL AS ad_subscription_date,&#13;&#10;    fus_subscription_type_id AS ad_subscription_type_id,&#13;&#10;    fus_accountnum AS ad_accountnum,&#13;&#10;    (CASE WHEN fus_call_direction_id IN (1,3) &#13;&#10;&#9;&#9;THEN fus_bno &#13;&#10;&#9;&#9;ELSE fus_ano END) ad_msisdn,&#13;&#10;    fus_imsi AS ad_imsi,&#13;&#10;    fus_call_direction_id AS ad_call_direction_id,&#13;&#10;    fus_calltype AS ad_calltype,&#13;&#10;    fus_terminationreason AS ad_terminationreason,&#13;&#10;    fus_mobilecountrycode AS ad_mobilecountrycode,&#13;&#10;    fus_mobilenetworkcode AS ad_mobilenetworkcode,&#13;&#10;    fus_cell_site_id AS ad_cell_site_id,&#13;&#10;    fus_service_type_id AS ad_service_type_id,&#13;&#10;    fus_country_operator_id AS ad_country_operator_id,&#13;&#10;&#9;--ad_eris_country_operator_id column for Oman&#13;&#10;&#9;NULL AS ad_eris_country_operator_id,&#13;&#10;    fus_origzone_id AS ad_origzone_id,&#13;&#10;    fus_termzone_id AS ad_termzone_id,&#13;&#10;    dev_eventid_base AS ad_eventid,&#13;&#10;&#9;0 AS ad_adjustment_type_id,&#13;&#10;&#9;0 AS ad_voucher_type_id,&#13;&#10;&#9;0 AS ad_counter_type_id,&#13;&#10;&#9;NULL AS ad_counter_id,&#13;&#10;&#9;0 AS ad_updatetype,&#13;&#10;&#9;SUM(fus_quantity) ad_total_quantity,&#13;&#10;&#9;0 AS ad_total_adjustment_amount,&#13;&#10;&#9;0 AS ad_total_valuediff,    &#13;&#10;&#9;SUM(fus_duration) AS ad_total_duration_actual,&#13;&#10;&#9;SUM(fus_total_duration_sec) AS ad_total_duration_sec,&#13;&#10;&#9;SUM(fus_total_duration_min) AS ad_total_duration_min,&#13;&#10;&#9;SUM(fus_chargedduration) AS ad_total_duration_charged,&#13;&#10;&#9;SUM(CASE WHEN fus_service_type_id =1 AND (fus_usedcountervaluestime + fus_usedcountervaluesinteger) &gt; fus_duration &#13;&#10;&#9;&#9;THEN (fus_usedcountervaluestime + fus_usedcountervaluesinteger) &#13;&#10;&#9;&#9;ELSE fus_duration END) AS ad_total_duration,&#13;&#10;&#9;SUM(CASE WHEN fus_service_type_id =1 AND (fus_usedcountervaluestime + fus_usedcountervaluesinteger) &gt; fus_duration &#13;&#10;&#9;&#9;THEN (fus_usedcountervaluestime + fus_usedcountervaluesinteger) &#13;&#10;&#9;&#9;ELSE fus_duration END) -&#13;&#10;&#9;SUM(CASE WHEN fus_service_type_id =1&#13;&#10;&#9;&#9;THEN (fus_usedcountervaluestime + fus_usedcountervaluesinteger) &#13;&#10;&#9;&#9;ELSE 0 END) AS ad_total_duration_payg,&#13;&#10;&#9;0 AS ad_total_duration_counter,&#13;&#10;&#9;0 AS ad_total_duration_counter_expired,&#13;&#10;&#9;0 AS ad_total_duration_counter_reset,&#13;&#10;&#9;SUM(CASE WHEN fus_service_type_id =2 THEN fus_quantity ELSE 0 END) AS ad_total_sms,&#13;&#10;&#9;SUM(CASE WHEN fus_service_type_id =2 AND fus_usedcountervaluesinteger=0&#13;&#10;&#9;&#9;&#9;THEN 1 ELSE 0 END) AS ad_total_sms_payg,&#13;&#10;&#9;0 AS ad_total_sms_counter,&#13;&#10;&#9;0 AS ad_total_sms_counter_expired,&#13;&#10;&#9;0 AS ad_total_sms_counter_reset,&#13;&#10;&#9;SUM(fus_datavolumereceived) AS ad_total_data_volume_received,&#13;&#10;&#9;SUM(fus_datavolumesent) AS ad_total_data_volume_sent,&#13;&#10;&#9;SUM(fus_usedquota) AS ad_total_data_volume_actual,&#13;&#10;&#9;SUM(CASE WHEN fus_service_type_id =5 AND fus_usedcountervaluesdatavolume &gt; fus_usedquota &#13;&#10;&#9;&#9;&#9; THEN fus_usedcountervaluesdatavolume &#13;&#10;&#9;&#9;&#9;ELSE fus_usedquota END ) AS ad_total_data_volume,&#13;&#10;&#9;SUM(CASE WHEN fus_service_type_id =5 AND fus_usedcountervaluesdatavolume &gt; fus_usedquota &#13;&#10;&#9;&#9;&#9;THEN fus_usedcountervaluesdatavolume &#13;&#10;&#9;&#9;&#9;ELSE fus_usedquota END ) -&#13;&#10;&#9;SUM(CASE WHEN fus_service_type_id =5&#13;&#10;&#9;&#9;&#9;THEN fus_usedcountervaluesdatavolume &#13;&#10;&#9;&#9;&#9;ELSE 0  END)  AS ad_total_data_volume_payg,&#13;&#10;&#9;0 AS ad_total_data_volume_counter,&#13;&#10;&#9;0 AS ad_total_data_volume_expired,&#13;&#10;&#9;0 AS ad_total_data_volume_reset,&#13;&#10;&#9;SUM(ZEROIFNULL(fus_cost) - ZEROIFNULL(fad_amount)) AS ad_total_revenue_gross,&#13;&#10;&#9;SUM(ZEROIFNULL(fus_cost) - ZEROIFNULL(fad_amount)) AS ad_total_revenue_received,&#13;&#10;&#9;&#9;--ad_dpo_id column for Oman peak off-peak&#13;&#10;&#9;NULL AS ad_dpo_id,&#13;&#10;&#9;fus_additionalinfo2 as ad_package_id&#13;&#10;FROM&#13;&#10;(&#13;&#10;(SELECT&#13;&#10;&#9;fus_date,&#13;&#10;&#9;fus_internalreferenceid,&#13;&#10;&#9;fus_subscription_type_id,&#13;&#10;&#9;fus_accountnum,&#13;&#10;&#9;ZEROIFNULL(CASE WHEN (fus_call_direction_id IN (1,3) AND  regexp_like(fus_bno,'\\D')=1) THEN 0 ELSE regexp_replace(left(fus_bno,18),'\\D','')::int END) AS fus_bno,&#13;&#10;&#9;ZEROIFNULL(CASE WHEN (fus_call_direction_id NOT IN (1,3) AND regexp_like(fus_ano,'\\D')=1) THEN 0 ELSE regexp_replace(left(fus_ano,18),'\\D','')::int END) AS fus_ano,&#13;&#10;&#9;fus_imsi,&#13;&#10;&#9;fus_call_direction_id,&#13;&#10;&#9;fus_calltype,&#13;&#10;&#9;fus_terminationreason,&#13;&#10;&#9;fus_mobilecountrycode,&#13;&#10;&#9;fus_mobilenetworkcode,&#13;&#10;&#9;fus_cell_site_id,&#13;&#10;&#9;fus_service_type_id,&#13;&#10;&#9;fus_country_operator_id,&#13;&#10;&#9;fus_origzone_id,&#13;&#10;&#9;fus_termzone_id,&#13;&#10;&#9;dev_eventid_base,&#13;&#10;&#9;ZEROIFNULL(fus_quantity) AS fus_quantity,&#13;&#10;&#9;ZEROIFNULL(fus_duration) AS fus_duration,&#13;&#10;&#9;CEIL(ZEROIFNULL(fus_duration)/1000) AS fus_total_duration_sec,&#13;&#10;&#9;CEIL(ZEROIFNULL(fus_duration)/60000) AS fus_total_duration_min,&#13;&#10;&#9;ZEROIFNULL(fus_chargedduration) AS fus_chargedduration,&#13;&#10;&#9;ZEROIFNULL(fus_usedcountervaluestime) AS fus_usedcountervaluestime,&#13;&#10;&#9;ZEROIFNULL(fus_usedcountervaluesinteger) AS fus_usedcountervaluesinteger,&#13;&#10;&#9;ZEROIFNULL(fus_datavolumereceived) AS fus_datavolumereceived,&#13;&#10;&#9;ZEROIFNULL(fus_datavolumesent) AS fus_datavolumesent,&#13;&#10;&#9;ZEROIFNULL(fus_usedquota) AS fus_usedquota,&#13;&#10;&#9;ZEROIFNULL(fus_usedcountervaluesdatavolume) AS fus_usedcountervaluesdatavolume,&#13;&#10;&#9;ZEROIFNULL(fus_cost) AS fus_cost,&#13;&#10;&#9;fus_additionalinfo2&#13;&#10;FROM&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.nobill_fact_usage a&#13;&#10;&#9;left join &quot;+context.connection_verticadb_schema_prod+&quot;.nobill_dim_event b&#13;&#10;&#9;on a.fus_event_id=b.dev_event_id&#13;&#10;WHERE&#13;&#10;&#9;fus_date = '&quot;+context.vLoadDate+&quot;'&#13;&#10;ORDER BY&#13;&#10;&#9;fus_internalreferenceid )FUS&#13;&#10;LEFT JOIN&#13;&#10;(SELECT&#13;&#10;&#9;fad_amount,&#13;&#10;&#9;fad_referenceid&#13;&#10;FROM&#13;&#10;&#9;&quot;+context.connection_verticadb_schema_prod+&quot;.nobill_fact_adjustment&#13;&#10;WHERE&#13;&#10;&#9;fad_date = '&quot;+context.vLoadDate+&quot;'&#13;&#10;ORDER BY&#13;&#10;&#9;fad_referenceid )ADJ&#13;&#10;ON&#13;&#10;&#9;FUS.fus_internalreferenceid = ADJ.fad_referenceid)&#13;&#10;GROUP BY&#13;&#10;&#9;fus_date,&#13;&#10;&#9;fus_subscription_type_id,&#13;&#10;&#9;fus_accountnum,&#13;&#10;&#9;(CASE WHEN fus_call_direction_id IN (1,3) &#13;&#10;&#9;&#9;THEN fus_bno &#13;&#10;&#9;&#9;ELSE fus_ano END),&#13;&#10;&#9;fus_imsi,&#13;&#10;&#9;fus_call_direction_id,&#13;&#10;&#9;fus_calltype,&#13;&#10;&#9;fus_terminationreason,&#13;&#10;&#9;fus_mobilecountrycode,&#13;&#10;&#9;fus_mobilenetworkcode,&#13;&#10;&#9;fus_cell_site_id,&#13;&#10;&#9;fus_service_type_id,&#13;&#10;&#9;fus_country_operator_id,&#13;&#10;&#9;fus_origzone_id,&#13;&#10;&#9;fus_termzone_id,&#13;&#10;&#9;dev_eventid_base,&#13;&#10;fus_additionalinfo2&#13;&#10;&quot;"/>
